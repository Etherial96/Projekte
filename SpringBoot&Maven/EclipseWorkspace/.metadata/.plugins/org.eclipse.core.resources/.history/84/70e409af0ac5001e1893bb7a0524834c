var wood = document.querySelector('.wood');

    window.addEventListener('scroll', function () {
	        var rect = wood.getBoundingClientRect();
	        if (rect.bottom <= 0) {	
	            document.querySelector('.stickimenu').style.top = 0;
	            document.querySelector('.stickichatbot').style.position = 'sticky';
	            document.querySelector('.stickichatbot').style.bottom = 0;
	            document.querySelector('.stickichatbot').style.right = 0;
	            document.querySelector('.stickichatbot').style.left = 'auto';
	            //document.querySelector('.navbar').style.position = "sticky";
	        }
	        if (rect.bottom >= 0) {	
				document.querySelector('.stickichatbot').style.position = 'fixed';
	            document.querySelector('.stickichatbot').style.top = 0;
	            document.querySelector('.stickichatbot').style.right = null;
	            document.querySelector('.stickichatbot').style.left = null;
			}
	        
    });